{
  "name": "nft-share-platform-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/experimental": "^5.5.0",
    "@ethersproject/providers": "^5.5.3",
    "@ethersproject/units": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.22",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@walletconnect/ethereum-provider": "^1.7.1",
    "@web3-react/core": "^8.0.10-beta.0",
    "@web3-react/eip1193": "^8.0.5-beta.0",
    "@web3-react/empty": "^8.0.3-beta.0",
    "@web3-react/metamask": "^8.0.6-beta.0",
    "@web3-react/network": "^8.0.5-beta.0",
    "@web3-react/types": "^8.0.3-beta.0",
    "@web3-react/url": "^8.0.5-beta.0",
    "@web3-react/walletconnect": "^8.0.8-beta.0",
    "@web3-react/walletlink": "^8.0.10-beta.0",
    "copyfiles": "^2.4.1",
    "graphql": "^16.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "rimraf": "^3.0.2",
    "semantic-ui-css": "git+https://github.com/Semantic-Org/Semantic-UI-CSS.git",
    "semantic-ui-react": "^2.1.2",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "COMMENT-semantic-ui-css-as-git-repo": {
    "text": "semantic-ui-css above is installed as a git repo to fix an error until the fixed version is released as npm package - see https://github.com/Semantic-Org/Semantic-UI-CSS/issues/75"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000/graphql graphql-schema.json",
    "schema:codegen": "apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/*.ts --tagName=gql --addTypename --globalTypesFile=src/types/graphql-global-types.ts types",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "contracts:clean": "rimraf src/typechain-types",
    "contracts:update": "npm run contracts:clean && cd ../nft-share-platform-contracts && npm run build && copyfiles \"./typechain-types/**\" \"../nft-share-platform-frontend/src\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
